{
  "$id": "event.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Event",
  "description": "Event occurring during a battle",
  "extends": {
    "$ref": "message.json"
  },
  "properties": {
    "turnNumber": {
      "description": "The turn number in current round when event occurred",
      "type": "integer"
    }
  },
  "required": [
    "turnNumber"
  ]
}
